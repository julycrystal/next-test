{
  "name": "nextjs-playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "standard": "standard -v 'src/**/*.js' 'src/**/*.jsx' '__tests__/**/*.js' '__tests__/**/*.jsx'",
    "test": "jest ./__tests__ --testPathIgnorePatterns support/*",
    "unit-test": "jest ./__tests__/unit",
    "int-test": "jest ./__tests__/integration",
    "dev": "next ./src",
    "build": "next build ./src",
    "start": "next start ./src -p $NEXTJS_BIND_PORT",
    "mockserver": "MOCK_HEADERS=Authorization mockserver -p 9000 -m '__tests__/support/mocks'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willianantunes/nextjs-playground.git"
