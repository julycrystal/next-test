  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "bootstrap-css-only": "^4.3.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsdom": "^15.2.1",
    "logplease": "^1.2.15",
    "next": "^9.1.6",
    "next-redux-wrapper": "^4.0.1",
